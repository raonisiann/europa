EXEC_FILE_NAME=$(BIN_DIR)\europa.exe
C_FILES=$(SRC_DIR)\main.c $(SRC_DIR)\lexer.c $(SRC_DIR)\parser.c $(SRC_DIR)\ast.c $(SRC_DIR)\core\*.c $(SRC_DIR)\debug\*.c $(SRC_DIR)\io\*.c $(SRC_DIR)\europa\*.c
INCLUDE_DIR=/I$(SRC_DIR)\include /I$(SRC_DIR)\core\include /I$(SRC_DIR)\debug\include /I$(SRC_DIR)\io\include /I$(SRC_DIR)\europa\include

all: 
    echo $(MAKEDIR)
    echo $(BIN_DIR)
    $(CC) /nologo /TC $(C_FILES) $(INCLUDE_DIR) /link /out:$(EXEC_FILE_NAME)

